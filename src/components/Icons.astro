---
import { mapIcons, IconName } from "@/icons/mapIcons"

interface Props {
  name: IconName
  dark?: boolean
}

const { name, dark } = Astro.props

const iconSvg = dark ? mapIcons.DARK[name] : mapIcons[name]

if (!iconSvg) {
  console.error(`Icon "${name}" not found`)
}
---

<Fragment set:html={iconSvg} />
